type hal_mlipay_default, domain;
type hal_mlipay_default_exec, exec_type, file_type, vendor_file_type;

init_daemon_domain(hal_mlipay_default)

allow hal_mlipay_default hal_mlipay_default_exec:file execute_no_trans;
allow hal_mlipay_default hwservicemanager_prop:file { read open getattr map };
allow hal_mlipay_default hwservicemanager:binder call;
allow hal_mlipay_default init:unix_stream_socket connectto;
allow hal_mlipay_default property_socket:sock_file write;
allow hal_mlipay_default teei_client_device:chr_file { read write open };
